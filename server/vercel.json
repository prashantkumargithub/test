{
    "version": 2,
    "builds": [
      { "src": "index.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/index.js" },
      { "src": "/Uploads/(.*)", "dest": "/index.js" },
      { "src": "/image/(.*)", "dest": "/index.js" },
      { "src": "/file/(.*)", "dest": "/index.js" },
      { "src": "/(.*)", "dest": "/index.js" }
    ]
  }
  